{
  "name": "@blog/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "contentlayer2 build --clearCache && next dev",
    "build": "contentlayer2 build && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next .turbo node_modules .contentlayer",
